.base_mainContent {
    padding:0;
}

.content {
	min-height: 708px;
	box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);
	overflow: hidden;
	padding-bottom: 110px;
}
.mainPackage{
	margin: 20px;
	height: 160px;
	background-color: #f6f8fa;
  	border: solid 1px #efefef;
}

.packageImg{
	float: left;
	padding: 20px;
	width: 200px;
}
.packageInfo{
	padding: 20px 0;
	height: 160px;
	overflow: hidden;
}
.packageTitle{
	display: inline-block;
	width: 300px;
	line-height: 22px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	font-size: 16px;
	color: #353535;
}
.toolBox{
	float: right;
	margin-right: 20px;
}
.toolBox>ul>li{
	cursor: pointer;
	color: #2a75ed;
	list-style: none;
	float: left;
}
.toolBox>ul>li>a{
	color: #2a75ed;
}

.packageDesc{
	display: inline-block;
	width: 300px;
	line-height: 18px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	font-size: 14px;
	color: #888888;
}
.package_price{
	margin: 15px 0;
	font-size: 14px;
	color: #ff8f2c;
}
.packageResCount{
	color: #888888;
	float: right;
	margin-top: 20px;
}
.packageNumber>p{
	display: inline-block;
	line-height: 22px;
	margin: 0;
	font-size: 14px;
	text-align: justify;
	color: #353535;
}
.packageNumber>p:first-child{
	margin-right: 30px;
}
.packageNumber>p span{
	font-size: 16px;
	margin-left: 5px;
}

.packageNumber .show_state {
	float: right;
	color: #888888;
	margin-right: 20px;
}

.contentHead{
    margin: 20px;
	height: 36px;
}
.txtArea{
	line-height: 20px;
	height: 20px;
	font-size: 14px;
	text-align: justify;
	color: #353535;
	margin: 0 0 10px 20px;
}
.txtArea span{
	color: #888;
}

.addBtn{
	position: relative;
}
/*.addBtn:hover .addDown{
	display: block;
}*/
.addBtn .addDown{
	left: 0;
	top: 36px;
	width: 90px;
}
.addBtn .addDown li{
	width: 88px;
}
.addBtn .addDown li a{
	display: inline-block;
	color: #2a75ed;
	width: 100%;
	line-height: 34px;
}


.downshow{
	display: none;
	position: absolute;
	z-index: 100;
	padding: 0;
	border-radius: 2px;
	background-color: #ffffff;
	border: solid 1px #deeeff;
	color: #2a75ed;
}
.downshow li{
	float: left;
	list-style: none;
	line-height: 34px;
	font-size: 12px;
	text-align: center;
	margin:0;
	padding: 0;
}
.downshow li:hover{
	background-color: #f6faff;
}
.selector_resource_type {
	width: 70px;
	float: left;
	padding: 0 0 0 10px;
	margin-right: 10px;
}

.searchAllBtn {
	float: left;
}

.contentNoneTip {
    width: 100%;
    height: 20px;
    text-align: center;
    margin-top: 20px;
    color: #888888;
    font-size: 14px;
}
.singleItem{
	margin: 0 20px;
	padding: 20px 0;
	border-bottom: 1px solid #f2f2f2;
	background-repeat: no-repeat;
	background-position: 5px center;
	background-size: 24px;
}
.singleItem:first-child{
	border-top: 1px solid #f2f2f2;
}
.singleItem:hover{
	background-color: #f6faff;
}

.singleItem>div{
	vertical-align: top;
	display: inline-block;
}
.tyepImgContent{
	line-height: 45px;
	margin-right: 20px;
}
.tyepImgContent img{
	vertical-align: middle;
}
.singleImg{
	width: 60px;
	vertical-align: top;
}
.singleImg img{ display: block; }

.singleTitle{
	width: 430px;
	margin-left: 10px;
}
.singleTitle .titleWrap{
	margin-bottom: 5px;
}
.singleTitle .titleWrap span{
	vertical-align: top;
	line-height: 20px;
	font-size: 14px;
	color: #666666;
}
.singleTitle .title{
	display: inline-block;
	max-width: 300px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}

.singleTitle .titleWrap .littleIco{
	display: inline-block;
	-webkit-user-select:none;
	-moz-user-select:none;
	-ms-user-select:none;
	user-select: none;
	cursor: default;
	margin-left: 6px;
	font-size: 12px;
	text-align: center;
	padding: 0 4px;
	line-height: 18px;
	border-radius: 2px;
}
.singleTitle .titleWrap .try{
	color: #65c131;
	border: solid 1px #65c131;
}
.singleTitle .titleWrap .singlePay{
	color: #ff8f2c;
	border: solid 1px #ff8f2c;
}
.singleTitle .titleWrap .hasTop{
	color: #2a75ed;
	border: solid 1px #2a75ed;
}
.singleTitle .time {
	line-height: 20px;
	font-size: 12px;
	color: #b2b2b2;
}
.singlePlayNumber {
	position: relative;
}
.playNumber{
	width: 115px;
	line-height: 45px;
}
.aliveNumber{
	width: 130px;
}

.singleAliveNumber {
	margin-left: 10px;
	position: relative;
	line-height: 45px;

}

.singleAliveNumber:hover
.resource_details {
	display: block;
}


/*********** 显示详细信息 - start ***********/
.resource_details {
	display: none;
	position: absolute;
	top: 35px;
	left: 87px;
	width: 155px;
	padding: 5px 0;
	z-index: 100;
	border-radius: 2px;
	background-color: #ffffff;
	border: solid 1px #deeeff;
	color: #2a75ed;
	list-style: none;
}
.singlePlayNumber:hover .resource_details {
	display: block;
}
.detail_item {
	text-align: left;
	padding: 4px 0 5px 10px;
}
.detail_item:hover {
	background-color: #f6faff;
}
