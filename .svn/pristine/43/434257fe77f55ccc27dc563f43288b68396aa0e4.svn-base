.singleItem .toolBox{
	line-height: 45px;
}
.singleItem .singleToolBox{
	display: none;
	position: absolute;
	right: 21px;
}
.singleItem:hover .singleToolBox{
	display: inline-block;
}
.singleItem .singleState{
	font-size: 14px;
    display: inline-block;
    vertical-align: top;
    float: right;
	margin-top: 13px;
    line-height: 20px;
    width: 51px;
    background: #e64340;
    color: white;
    border-radius: 5px;
    font-size: 0.8em;
    text-align: center;
}
.singleItem:hover .singleState{
	display: none;
