}
.roleTypeRadioWrapper {
    display: inline-block;
    font-size: 14px;
    color: #243042;
}
.roleTypeRadio {
    height: 40px;
    line-height: 40px;
    cursor: pointer;
    float: left;
    margin-left: 10px;
}
.circleRadio {
    display: inline-block;
    position: relative;
    top: 2px;
    width: 14px;
    height: 14px;
    border-radius: 50%;
    border: 1px solid #dcdcdc;
    margin-right: 10px;
}
.radioActive {
    position: relative;
    border: 1px solid #2a75ed !important;
}
.radioActive:after {
    content: '';
    display: block;
    position: absolute;
    top: 1px;
    left: 1px;
    width: 10px;
    height: 10px;
    border-radius: 50%;
    background-color: #2a75ed;
}
.deleteSingleUser {
    height: 40px;
    line-height: 40px;
    font-size: 14px;
