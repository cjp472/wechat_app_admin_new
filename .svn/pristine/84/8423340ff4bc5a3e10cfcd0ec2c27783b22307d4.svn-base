.mainContent {
    width: 100%;
    padding: 0 20px 50px;
    height: auto;
}
.tableHeader {
    width: 100%;
    height: 40px;
    background: #f6f8fa;
}
.tableHeader > div {
    width: 20%;
    height: 40px;
    line-height: 40px;
    font-weight: bold;
    float: left;
}
.tableHeader > div:nth-child(1) {
    text-align: left;
    padding-left: 20px;
}
.tableHeader > div:nth-child(4) {
    width: 10%;
}
.tableHeader > div:nth-child(5) {
    width: 10%;
    text-align: center;
}
.tableHeader > div:nth-child(6) {
    text-align: center;
}

.tableContent {
    width: 100%;
    padding: 0;
}
.exerciseBookItem {
    width: 100%;
    height: 70px;
    padding: 20px 0;
    line-height: 30px;
    border-bottom: 1px solid #eeeeee;
}
.exerciseBookItem > div {
    width: 20%;
    height: 30px;
    line-height: 30px;
    float: left;
    text-align: left;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.exerciseBookItem > div:nth-child(1) {
    text-align: left;
    padding-left: 20px;
}
.exerciseBookItem > div:nth-child(4) {
    width: 10%;
}
.exerciseBookItem > div:nth-child(5) {
    width: 10%;
    text-align: center;
}

/*********************** 操作部分 -start *************************/
.exerciseBookItem .exerciseBookOperateArea {
    text-align: center;
}
.operateList {
    height: 30px;
