}
.windowHoverBox {
    display: block;
    position: absolute;
    bottom: 36px;
    left: 0;
    width: 100px;
    height: 0;
    padding: 0;
    margin: 0;
    overflow: hidden;
    -webkit-transition: all 0.3s ease;
    transition: all 0.3s ease;
}
.windowHoverBox > li {
    height: 36px;
    line-height: 36px;
    text-align: center;
    color: blue;
    cursor: pointer;
    border: 1px solid #00a0e9;
    border-bottom: none;
}
.confirmSaveRoleBtn {
    float: left;
}

