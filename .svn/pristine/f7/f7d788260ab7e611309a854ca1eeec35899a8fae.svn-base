
$(function(){
    recordList.init();
});

var recordList = {
    // 初始化的一些方法
    init: function(){
        $('body').on('click','#searchBtn',function(){
            // 获得搜索的值
            var search_value = $("#search_content").val();
            if($chosenShop.tabState === 'record'){
                $chosenShop.getNewPage('record',{'search_content':search_value});
            }
        })
    }
};