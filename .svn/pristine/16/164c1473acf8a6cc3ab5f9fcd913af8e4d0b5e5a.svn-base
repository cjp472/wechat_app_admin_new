/*图片预览与上传*/
.previewPic {
    float: left;
    width: 200px;
    height: 200px;
    background-position: center;
    background-repeat: no-repeat;
    background-color: #f7f8f9;
    border-radius: 2px;
    overflow: hidden;
    cursor: pointer;
}

.previewPic img {
    border: none;
    width: 100%;
    height: 100%;
}
.previewInfo{
    float: left;
}
.coverUpbtn {
    cursor: pointer;
    position: relative;
    margin-left: 20px;
    margin-right: 80px;
    overflow: hidden;
}
.coverUpbtn span {
    cursor: pointer;
}
.coverUpbtn input {
    cursor: pointer;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.coverUpTip {
    width: 300px;
    line-height: 20px;
    margin-left: 20px;
    margin-top: 10px;
    color: #b2b2b2;
}

input[disabled]{
    color: #c5c5c5;
}
/* 复选框的样式 */
input[type=checkbox] {
    opacity: 0;
    width: 0;
    height: 0
}

input[type=checkbox]+label {
    position: relative;
    padding-left: 30px;
    line-height: 36px!important;
    margin-bottom: 0;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    float: left;
    font-size: 14px;
    color: #666;
    user-select:none;
}

input[type=checkbox]+label:before {
    position: absolute;
    left: 0;
    top: 10px;
    cursor: pointer;
    display: inline-block;
    content: '\a0';
    background: #fff;
    color: #fff;
    border-radius: 3px;
    border: 2px solid #999;
    width: 15px;
    height: 15px;
    line-height: 12px;
    text-align: center
}

input[type=checkbox]:checked+label:before {
    content: '\2713';
    background: #2a75ed;
    border-color: #2a75ed
}

