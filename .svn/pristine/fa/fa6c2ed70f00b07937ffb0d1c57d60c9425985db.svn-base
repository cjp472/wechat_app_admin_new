}


.loadingPartial .loadingSContent {
    position: absolute;
    width: 100px;
    height: 90px;
    top: 50%;
    left: 50%;
    margin-left: -50px;
    margin-top: -50px;
}

.loadingPartial .loadingSmallContent {
    position: absolute;
    width: 24px;
    height: 24px;
    top: 50%;
    left: 50%;
    margin-left: -12px;
    margin-top: -12px;
    overflow: hidden;
}
.loadingPartial .circular {
    width: 42px;
    height: 42px;
    -webkit-animation: loading-rotate 2s linear infinite;
    animation: loading-rotate 2s linear infinite;
}

.loadingPartial .circularSmall {
