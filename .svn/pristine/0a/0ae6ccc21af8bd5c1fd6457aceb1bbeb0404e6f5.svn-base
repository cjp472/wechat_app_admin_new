.noticeHeader {
    width: 100%;
    height: 50px;
    background: #fafbfc;
    border-bottom: 1px solid #eeeeee;
    line-height: 50px;
    padding-left: 20px;
    font-size: 14px;
    box-sizing: border-box;
}

.noticeListContent {
    width: 100%;
    height: auto;
    padding: 20px;
    box-sizing: border-box;
}

.noticeListPart {
    width: 100%;
    height: auto;
    cursor: pointer;
    border-bottom: 1px solid #eeeeee;
}

.noticeListPart:hover{
    background: #f6faff;
}
.noticeListPart1 {
    width: 100%;
    box-sizing: border-box;
    padding-left: 10px;
    padding-right: 10px;
    height: auto;
    overflow: hidden;
}

.noticePartTitle{
    float: left;
    line-height: 60px;
    text-align: left;
}

.noticeListTime{
    float: right;
    line-height: 60px;
    text-align: right;
    color: #666666;
}

.listSdIcon{
    display: inline-block;
    position: relative;
    margin-left: 10px;
    top:2px;
    left:4px;
    border-bottom: 6px solid transparent;
    border-top: 6px solid transparent;
    border-right: 6px solid #c1c4ca;
    transition: 0.3s all ease;
}

.unreadPoint {
    position: relative;
    display: inline-block;
    width: 6px;
    height: 6px;
    border-radius: 3px;
    top: -2px;
    left: 8px;
    background: #fb6161;
}

.noticePartContent{
    width: 100%;
    line-height: 20px;
    padding: 0 20px 20px 20px;
    color:#666666;
}