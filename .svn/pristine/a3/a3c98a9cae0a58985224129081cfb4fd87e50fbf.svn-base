    z-index: 100;
    padding: 0;
    border-radius: 2px;
    background-color: #ffffff;
    border: solid 1px #deeeff;
    color: #2a75ed;
}

.more_operate_wrapper:hover .more_operate_wrapper {
    display: block;
}

.hover_tool_box_2 .more_operate_wrapper li {
    float: left;
    list-style: none;
    line-height: 27px;
    height: 27px;
    font-size: 12px;
    text-align: center;
    margin: 0;
    padding: 0;
    float: inherit;
}

/* 下架标签 */
.packageHasDown {
    width: 51px;
    height: 20px;
    line-height: 20px;
    position: absolute;
    right: 15px;
    bottom: 15px;
    margin-right: 20px;
    background: #e64340;
    color: white;
    border-radius: 5px;
    font-size: 0.8em;
    display: inline-block;
    text-align: center;
}
.package_list_item:hover .packageHasDown {
    display: none;
}
