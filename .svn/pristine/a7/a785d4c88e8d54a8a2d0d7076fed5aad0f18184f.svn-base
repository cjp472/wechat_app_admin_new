/*********** 加载动画 - end ***********/
.isDown{
    line-height: 50px;
    text-align: center;
    color: #888888;
}
.isDownAnimation {
    color: #666;
    -webkit-animation: shake 1s linear infinite alternate;
    -moz-animation: shake 1s linear infinite alternate;
    animation: shake 1s linear infinite alternate;
}
@-webkit-keyframes shake{
    from{
        opacity: 0;
    }
    to{
        opacity: 1;
    }
}
@-moz-keyframes shake{
    from{
        opacity: 0;
    }
    to{
        opacity: 1;
    }
}
@keyframes shake{
    from{
        opacity: 0;
    }
    to{
        opacity: 1;
    }
}

