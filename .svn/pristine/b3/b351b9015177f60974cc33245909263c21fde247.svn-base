/***********************功能配置页********************************/

.contentBox{
    margin:40px 30px 40px ;
}
.pageTitle{
    font-size: 16px;
    font-weight: 600;
    margin-bottom:25px;
}
.subTitle{
    display: inline-block;
    padding-top:7px;
    width:155px;
}
.selectBox{
    display: inline-block;
    vertical-align: top;
}
.subTip{
    color: #666666;
}
#remindCheck{
    padding-left: 20px;
    font-size: 12px;
}