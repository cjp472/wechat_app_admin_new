/* 精选与隐藏 */
span.tips{
    display: inline-block;
    line-height: 18px;
    letter-spacing: -0.2px;
    padding: 0 5px;
    border-radius: 2px;
    font-size: 12px;
    color: #fff;
}
/*span.tips.hidded{
    background-color: #E64340;
}*/
span.tips.quality{
    background-color: #2A75ED;
}

.packageName{
    padding: 0;
    margin: 0;
    max-width: 300px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
/* 表格中操作部分的样式 */
.toolTh{
    width: 245px;
}
.dynamicToolTh{
    width: 125px;
}
.caret{
    margin-left: 5px;
}
.table>tbody>tr .td_left{
    text-align: left;
}
.toolBox{
    display: inline-block;
}
.toolBox ul{
    margin: 0;
    padding: 0;
}
.toolBox ul li {
    line-height: 34px;
    list-style: none;
    float: left;
    color: #2a75ed;
    cursor: pointer;
}

.toolBox ul li.operate {
    line-height: 34px;
    cursor: pointer;
    color: #2a75ed;
    list-style: none;
    float: left;
}
.toolBox ul li a{
    color: #2a75ed;
    display: block;
}
.moreBtn {
    position: relative;
}
.moreBtn:hover .moreTool {
    display: block;
}
.moreBtn .moreTool {
    display: none;
    position: absolute;
    top: 33px;
    left: -14px;
    width: 80px;
    z-index: 100;
    padding: 0;
    border-radius: 2px;
    background-color: #fff;
    border: solid 1px #deeeff;
    color: #2a75ed;
}
.moreBtn .moreTool li {
    width: 78px;
    float: left;
    list-style: none;
    line-height: 34px;
    font-size: 12px;
    text-align: center;
    margin: 0;
    padding: 0;
}
.moreBtn .moreTool li:hover {
    background-color: #f6faff;
}

/*********************** 动态列表 - start *************************/
.baseManageTab li {
    padding: 0 10px;
    margin: 0 10px;
}
.baseManageTab a {
    margin: 0;
    padding: 0;
    font-size: 14px;
}
.baseActiveTab a {
    color: #2a75ed;
}

.contentNoneTip {
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    margin-top: 50px;
    color: #888;
    font-size: 16px;
}

/*********************** 动态列表 - end *************************/

