.content {
    min-height: 700px;
    padding: 20px 20px 50px 20px;
    box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);
    overflow: hidden;
}
.communityName {
    margin-bottom: 20px;
    font-size: 16px;
    color: #353535;
}
.content .baseManageTab {
    height: 50px;
    margin-bottom: 20px;
}
.content .baseManageTab a {
    line-height: 50px;
}
/* 顶部 */
.top_area {
    height: 36px;
    margin-bottom: 20px;
}


/* 查询相关样式 */
.right_operate {
    height: 36px;
    float: right;
}
.selector {
    float: left;
    width: 95px;
    padding: 0 0 0 10px;
    margin-right: 10px;
    border-radius: 2px;
}
.inputSearchAll {
    width: 240px;
    float: left;
    margin-right: 10px;
}

.searchAllBtn {
    width: 80px;
}

/* 表格相关样式 */
.table tr .td_left{
    padding-left: 18px;
}
.table tr .td_right{
    padding-right: 18px;
}
.table tr td img{
    width: 40px;
    height: 40px;
}
.table tr td img.userImg{
    border-radius: 50%;
}
.table tr td .username {
    display: inline-block;
    line-height: 40px;
    max-width: 140px;
    white-space: nowrap;
    overflow: hidden;
    vertical-align: middle;
    text-overflow: ellipsis;
    margin: 0;
}
.table tr th.textContent{
    width: 325px;
}
.dynamicTextContent {
    max-width: 298px;
    margin:0px !important;
    position: relative;
    line-height: 20px;
    max-height: 40px;
}
