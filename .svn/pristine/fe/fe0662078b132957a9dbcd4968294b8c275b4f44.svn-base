.base_mainContent {
    padding:0;
}

.content {
    min-height: 708px;
    box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);
    overflow: hidden;
}

.contentHead{
    margin: 20px;
	height: 36px;
}

.searchArea .selector{
	float: left;
	width: 78px;
	padding: 0 0 0 10px;
	margin-right: 10px;
	border-radius: 2px;
}
.searchArea .inputDefault{
	float: left;
	width: 240px;
    height: 36px;
    margin-right: 10px;
    border-radius: 2px;
}
.searchBtn{
	float: left;
	width: 80px;
}
.contentBody{
	padding: 0;
}
.listItem{
	display: block;
	overflow: hidden;
	height: 130px;
	padding: 20px 0;
	margin: 0 20px;
	background-color: #fff;
	border-bottom: 1px solid #eee;
	cursor: pointer;
}
.listItem:first-child{
	border-top: 1px solid #eee;
}
.listItem:hover{
	background-color: #f6faff;
}
.listItem:hover .toolBox{
	display: block;
}
.listItem .listImg{
	float: left;
	padding: 0 20px;
	line-height: 90px;
	width: 160px;
}
.listImg a {
	display: block;
}
.listItem .listInfo{
	overflow: hidden;
	height: 90px;
}
.listInfoTitle a {
	display: inline-block;
}
.listInfoTitle .title{
	width: 650px;
	height: 20px;
	display: inline-block;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	margin-bottom: 6px;
	font-size: 16px;
	color: #353535;
}
.distribute_target{
	width: 32px;
	height: 16px;
	border-radius: 2px;
	margin-left: 10px;
	background-color: #ff6d28;
	text-align: left;
	font-size: 10px;
	letter-spacing: -0.2px;
	color: #ffffff;
}

.toolBox{
	display: none;
	float: right;
	margin-right: 20px;
}
.toolBox ul li{
	cursor: pointer;
	color: #2a75ed;
	list-style: none;
	float: left;
}
.toolBox ul li a{
	display: block;
	color: #2a75ed;
}

.listInfoDesc{
	display: inline-block;
	width: 800px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	font-size: 14px;
	color: #666666;
	margin-bottom: 13px;
}

.listInfoState{
	float: right;
	font-size: 14px;
	color: #666666;
	margin-right: 20px;
}
.listInfoState .resCount{
	margin-right: 20px;
}
.listItem:hover .listInfoState{
	display: none;
}

.listInfoPrice .price {
	line-height: 20px;
	color: #ff8f2c;
}
.listInfoPrice .hasDown{
	line-height: 20px;
    margin-right: 20px;
	width: 51px;
	height: 20px;
	background: #e64340;
	color: white;
	border-radius: 5px;
	font-size: 0.8em;
	display: inline-block;
	text-align: center;
}


.contentNoneTip {
    width: 100%;
    height: 20px;
    text-align: center;
    margin-top: 20px;
    color: #888888;
    font-size: 14px;
}

.red_prompt {
	background-color: #629EFF;
}
.red_prompt_word {
	letter-spacing: 0.7px;
}

.red_prompt_word a {
	color: #629EFF;
	background: white;
	display: inline-block;
	height: 28px;
	line-height: 28px;
	border-radius: 2px;
	padding: 0 5px;
	margin: 0 10px;
	font-size: 12px;
}
.close_img_wrapper {
	background-color: #629EFF;
}