.orange-color {
    color: #f77849;
}

.record_left{
    padding-left: 10px;
    text-align: left;
}
.order_left {
    padding-left: 10px;
}
.order_input{
    width: 440px;
    alignment: center;
}
.recordsContent{
    position: relative;
    width: 100%;
    height: auto;
}
.order_name{
    width:200px;
}
.order_info{
    height: 75px;
    width: 1672px;
    color: #666;
    border-bottom: 1px solid #eee;
}

.img{
    height: 45px;
    width: 60px;
    vertical-align: middle;
}

.status_right{
    height:inherit;
    text-align: right;

}
.recordTable{
    width: 100%;
}
.recordTable > thead > tr {
    background-color: #f6f8fa;
    height: 40px;

}

.record_center{
    text-align: center;
}
.record_right {
    text-align: right;
    padding-right: 10px;
}

.goodsSearch {
    margin-top: 20px;
    overflow: hidden;
    margin-bottom: 20px;
}

.distribute_name{
    display: inline-block;
    vertical-align: middle;
    padding-left: 10px;
    height: 45px;
    line-height: 45px;
    font-size: 14px;
    white-space: nowrap;
    /* 不换行 */
    overflow: hidden;
    /* 内容超出宽度时隐藏超出部分的内容 */
    text-overflow: ellipsis;
    /* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/
}
.order_img{
    font-size: 0;
}
.order_img img{
    width: 60px;
    height: 45px;
}
.searchContent{
    padding: 20px 0;
}

.goodsSearchBtn {
    width: 80px;
    margin-left: 10px;
}
.search_button .search_content{
    display: inline-block;
}

.my-input{
    width: 240px;
}

.contentNoneTip {
    text-align: center;
    margin-top: 60px;
}
