.revenueInformation{
    margin-top: 10px;
    margin-bottom: 10px;
    width: 100% ;
    height: 100px;
    border-width: 1px;
    border-style: none;
    border-top: 1px solid #ECECEC;
    border-bottom:1px solid #ECECEC
}
.ul-revenue{
    margin:auto;
    width: 100%;
    height: 100px;
}
.purchaseTable{
    width: 100%;
}
.purchaseTable th{
    font:normal bold normal 14px/1.5em 'Microsoft YaHei';
    letter-spacing: -0.3px;
    color: #353535;
    height: 50px;
    border-top:1px solid #ececec!important;
    border-bottom:1px solid #ececec!important;
    text-align: center;
}
/*第一栏比较宽，而且全部靠左*/
.purchaseTable th:nth-child(1)
{
    max-width: 50px;
    text-align: left;
}
/*体*/
.pay_tbody tr
{
    border-bottom:1px solid #ececec!important;
    text-align: center;
    height: 70px;
    font:normal normal normal 14px/1.5em 'Microsoft YaHei';
    letter-spacing: -0.2px;
    color: #3e3e3e;
}
/*所有的第一个td左对齐*/
td:nth-child(1)
{
    max-width: 50px;
    text-align: left;
}
/*头像及昵称*/
.purchaseTable img
{
    width: 50px;
    height: 50px;
    border-radius: 2px;
    margin: 10px 11px 10px 0;
    cursor: pointer;
}
.imgRight
{
    font:normal normal normal 14px/1.5em 'Microsoft YaHei';
    letter-spacing: -0.3px;
    /*color: #576b95;*/
}
/*购买人数*/
.sumPayUser{
    float: left;
    width: 25%;
    height: 100%;
    line-height: 34px;
}
/*搜索*/
.searchArea
{
    float: right;
    width: 100%;
    height:34px;
    margin: 10px 0 20px 0;
}
.searchSelectArea
{
    float: right;
    margin-right:10px;
    margin-left: 10px;
}
/*计费说明*/
.paydoc{
