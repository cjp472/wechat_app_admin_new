}
.mytable > tbody > tr{
    height: 55px;
}
.mytable > thead > tr{
    height: 40px;
}
.mytable > thead > tr > th {
    border-bottom: none;
}
.mytable > tbody > tr > td {
    border-top: 1px solid #eee;
    line-height: 80px;
    height: 80px;
    padding: 0;
}
.recordTable > tbody > tr > td {
    line-height: 55px;
    height: 55px;
}
.recordTable > tbody > tr > td p{
    line-height: 55px;
    height: 55px;
}
.recordTable > tbody > tr{
	line-height: 55px;
    height: 55px;
}
.mytable tbody>tr img{
	width: 40px;
	height: 40px;
	vertical-align: middle;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
}
.tableContainer .mytable tr td .username{
	display: inline-block;
	line-height: 40px;
	max-width: 140px;
	white-space: nowrap;
	overflow: hidden;
	vertical-align: middle;
