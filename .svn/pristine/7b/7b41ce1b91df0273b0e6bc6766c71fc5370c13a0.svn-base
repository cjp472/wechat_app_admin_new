.biLevel{
	cursor: help;
	position: relative;
}
.biLevel:hover .invite{
	display: block;
}
.invite{
	display: none;
	position: absolute;
	right: -16px;
	top: 30px;
	font-size: 14px;
	line-height: 50px;
	width: 100px;
	text-align: center;
	border: solid 1px #e3e3e3;
	background-color: #fff;
    -webkit-box-shadow: 5px 5px 8px 0 rgba(0,0,0,.3);
    -moz-box-shadow: 5px 5px 8px 0 rgba(0,0,0,.3);
    box-shadow: 5px 5px 8px 0 rgba(0,0,0,.3);
}

.checkContent{
	position:relative;
}
.checkContent .checkBox{
	z-index: 100;
	display: none;
	position:absolute;
	top: 34px;
	right: 20px;
	width: 420px;
	padding: 10px;
	background-color:#fff;
	border: 1px solid #e3e3e3;
    -webkit-box-shadow: 5px 5px 8px 0 rgba(0,0,0,.3);
    -moz-box-shadow: 5px 5px 8px 0 rgba(0,0,0,.3);
    box-shadow: 5px 5px 8px 0 rgba(0,0,0,.3);
}
.checkContent .checkBox label{
	margin-right: 15px;
}
.checkContent .checkBox button{
	margin-right: 15px;
}
.checkContent .checkBox button.xeBtnDefault{
	margin-right: 0;
}
.contentNoneTip {
