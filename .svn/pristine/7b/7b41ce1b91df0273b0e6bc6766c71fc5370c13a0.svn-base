.table tr {
    text-align: center;
    color: #666666;
    background-color: #fff;
    font-size: 14px;
    line-height: 40px !important;
    border-bottom: 1px solid #eeeeee;
}
.mytable tr th{
	font-size: 14px;
	font-weight: 600;
