/****************************先除去base_mainContent的padding和设置背景********************/
.base_mainContent
{
    padding:0;
    background-color: #efeff4;
}
/**********************************共用标题区 - 已经删除，改为模板 ************************************/

/*每个块*/
.content
{
    width: 100%;
    background-color: #ffffff;
    box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);
    overflow: hidden;
    margin: 10px 0 0;
}

.content.first{
    margin-top: 0;
}

/*块内的标题部分*/
.contentTitle
{
    height: 50px;
    font-family: PingFangSC;
    font:normal normal normal 16px/50px 'Microsoft YaHei';
    font-size: 16px;
    color: #243042;
    border-bottom: 1px solid #f2f2f2;
    padding-left: 20px;
    float: left;
}

.record_num{
    height: 50px;
    font:normal normal normal 16px/50px 'Microsoft YaHei';
    font-size: 12px;
    color: #243042;
    border-bottom: 1px solid #f2f2f2;
    padding-left: 6px;
    float: left;
}

.cost_explain{
    width: 56px;
    height: 50px;
    line-height: 50px;
    margin-right: 29px;
    float: right;
    color: #2a75ed;
    cursor: pointer;
}

/******************************共用标题区 - end ************************************/


