    margin-left: 10px;
    font-size: 12px;
}

/**************************************表格 table - start**********************************/
.table_resource_list {
    width: 100%;
}
thead {
    border-bottom: none;
}
.tr_head {
    height: 40px;
    line-height: 40px;
}

.tr_body td {
    padding: 20px 0;
    text-align: center;
    border-bottom: solid 1px #eeeeee;
    border-top: solid 1px #eeeeee;
    color: #666666;
}
.tr_body td:hover {
    padding: 20px 0;
    text-align: center;
    border-top: solid 1px #deeeff;
    border-bottom: solid 1px #deeeff;
}

.tr_body:hover {
    background: #f6faff;
    border-bottom: solid 1px #deeeff;
}

.tr_head .th_resource_name {
    text-align: left;
    padding-left: 20px;
}
.tr_head .th_resource_operate {
    width: 180px;
}
.tr_head .th_resource_num{
    text-align: left;
    padding-left: 25px;
    min-width: 70px;
}

.member_type_icon {
    width: 44px;
    height: 45px;
    padding: 11px 20px 10px 0;
    float: left;
}

.tr_head th {
    background: #f6f8fa;
    border: none;
    padding: 0;
    text-align: center;
    min-width: 60px;
}

/*********** 显示详细信息 - start***********/
.tr_body td.purchase_count_td {
    text-align: left;
    padding-left: 30px;
}

.arrow_down_icon {
    width: 10px;
    height: 6px;
    margin: 7px 0 7px 5px;
}

.td_relative_info {
    position: relative;
    max-width: 160px;
}
.td_relative_info .singleRelative {
    width: 100%;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.productCountBox {
    cursor: pointer;
}
.productCountBox .downIcon {
    width: 10px;
    height: 6px;
    position: relative;
    top: 0;
    left: 0;
}
.td_relative_info:hover .relevanceBox {
    display: block;
}
.relevanceBox {
    display: none;
    width: 260px;
    height: auto;
    z-index: 100;
    position: absolute;
    padding: 10px;
    top: 60px;
    left: 20px;
    border: 1px #eee solid;
    background: #fff;
    text-align: left;
}

.relevanceBox > div {
    line-height: 25px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.resource_show_state {
    text-align: center;

}
.resource_show_state div {
    width: 51px;
    height: 20px;
    background: #e64340;
    color: white;
    border-radius: 5px;
    font-size: 0.8em;
    line-height: 20px;
    display: inline-block;
}
.purchase_count_td {
    position: relative;
}
.resource_details {
    display: none;
    position: absolute;
    top: 65px;
    left: 50px;
    width: 155px;
    padding: 5px 0;
    z-index: 100;
    border-radius: 2px;
    background-color: #ffffff;
    border: solid 1px #deeeff;
    color: #2a75ed;
}
.purchase_count_td:hover .resource_details {
    display: block;
}
.detail_item {
    text-align: left;
    padding: 4px 0 5px 10px;
}
.detail_item:hover {
    background-color: #f6faff;
}
/*********** 显示详细信息 - end***********/

/**********************************表格 table - end****************************************/

