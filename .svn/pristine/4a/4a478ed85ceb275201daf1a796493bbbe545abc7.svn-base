}
div.tab{
    border-bottom:none;
    padding-left:20px;
    margin-left:-20px;
    box-sizing: content-box;
}
.downloadPop div.modal-body{
    width:660px;
}
.downloadPop div.modal-footer{
    padding-left:45px;
