

.base_mainContent img {
    width: 100%;
    height: 100%;
}

.selectItem {
    float: left;
    margin: 30px 0 0 20px;
    width: 245px;
    height: 118px;
    box-sizing: border-box;
    border: 1px solid #eeeeee;
    transition: 0.2s all ease;
    cursor: pointer;
}
.selectItem:hover {
    box-shadow: 0 6px 10px 0 rgba(0, 0, 0, 0.14), 0 1px 18px 0 rgba(0, 0, 0, 0.12), 0 3px 5px -1px rgba(0, 0, 0, 0.3);
}

.comment {
    margin-right: 20px;
}

.activity, .askAndQuestion, .smallCommunity, .exerciseBook {
    margin-right: 20px;
}

.PartIcon {
    width: 48px;
    height: 48px;
    margin-left: auto;
    margin-right: auto;
    margin-top: 20px;
}

.PartWord {
    width: 100%;
    height: 20px;
    line-height: 20px;
    margin-top: 10px;
    font-size: 14px;
    text-align: center;
}

.pageVessel{
    width: 100%;
    height: 100%;
    padding: 20px;
    overflow: auto;
}


