//2017-7-19 概况页改版 by Jervis

//顶部数据模块
.dataBox{
	padding:$basePadding;
	background-color: #fff;
	margin-bottom:40px;
	.myShop{
		.shopFitment{
			margin-right:$basePadding;
		}
		height: 56px;
		width: 100%;
		padding-bottom: $basePadding;
		border-bottom:1px solid #ececec;
		.content{
			float: left;
			/* img{
                vertical-align: top;
                width: 36px;
                height: 36px;
            } */
			p{
				line-height: 20px;
				display: inline-block;
				margin: 8px 8px 0 0;
				font-size: 18px;
				font-weight: 600;
				color: #353535;
			}
			div{
				display: inline-block;
			}
			.version{
				vertical-align: text-bottom;
				cursor: default;
				display: inline-block;
				border-radius: 1px;
				background-color: #5e99f7;
				color: #fff;
				font-size: 12px;
				padding: 0 5px;
				line-height: 20px;
				margin-right: $basePadding;
			}
			a{
				line-height: 20px;
			}
		}
		.btnContent{
			float: right;
			position: relative;
			.qrcodeArea{
				display: none;
				position: absolute;
				top: 50px;
				right: 0;
				overflow: hidden;
				width: 180px;
				height: 220px;
				padding: $basePadding 30px;
				border-radius: 2px;
				background-color: #ffffff;
				box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);
				border: solid 1px #dbdbdb;
				text-align: center;
				.text{
					line-height: 20px;
					font-size: 12px;
					color: #243042;
				}
				.codeContent{
					text-align: center;
					margin: 10px 0;
				}
				.href{
					a{text-decoration: underline !important; color: #2a75ed; font-size: 12px;}
				}
			}
		}

	}
	.shopData{
		.content{
			background: $baseColor;
			height: 100px;
			width: 100%;
		}
		.titleText{
			border-left:2px solid #2a75ed;
			padding-left: 9px;
		}
		.shopInfo{
			float: left;
			width: 60%;
			.content{
				padding: 15px 0;
				.item{
					float: left;
					height: 100%;
					width: 25%;
					text-align: center;
					border-left: 1px solid #eeeeee;
					.dataNum{
						margin-top: 13px;
						font-size: 24px;
						color: #3e3e3e;
					}
					.dataText{
						font-size: 12px;
						color: #888;
					}
				}
				.item:first-child{
					width: 20%;
					border-left: none;
				}
				.item:last-child{
					width: 30%;
				}
			}
		}
		.shopAccount{
			float: right;
			width: 38%;
			.content{
				.item{
					float: left;
					width: 50%;
					//height: 120px;
					.data{
						float: left;
						padding-left: 5%;
						width: 60%;
						height:70px;
						margin: 15px 0;
						text-align: center;
						.dataNum{
							font-size: 24px;
							color: #3e3e3e;
						}
						.dataText{
							margin: 0 0 7px;
							font-size: 12px;
							color: #888;
						}
					}
					.lastData{
						border-left: 1px solid #eee;
					}
					.bottomBtn{
						//display:block;
						margin: 11px 9% 35px 0;
						float: right;
						width: 44px;
						height: 22px;
						border-radius: 2px;
						line-height: 22px;
						text-align: center;
						font-size: 10px;
					}
					.recharge{
						background: #2a75ed;
						//border: solid 1px #2a75ed;
						color: #fff;
					}
					.record{
						background: #fafbfc;
						border: solid 1px #e5e7eb;
						color: #353535;
					}
				}
			}
		}
	}
}

