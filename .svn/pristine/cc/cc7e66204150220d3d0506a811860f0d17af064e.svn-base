    cursor: pointer;
}

tbody >tr > td{
    overflow: hidden;
    height: 60px;
}
tbody >tr > td:nth-of-type(1),thead >tr > th:nth-of-type(1){
    width: 5%;
}
tbody >tr > td:nth-of-type(2),thead >tr > th:nth-of-type(2){
