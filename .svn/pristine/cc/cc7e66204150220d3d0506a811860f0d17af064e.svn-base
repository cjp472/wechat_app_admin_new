}
tbody >tr > td:nth-of-type(4),thead >tr > th:nth-of-type(4){
    width: 10%;
}
tbody >tr > td:nth-of-type(5),thead >tr > th:nth-of-type(5){
    width: 15%;
}
tbody >tr > td:nth-of-type(6),thead >tr > th:nth-of-type(6){
    width: 10%;
}
tbody >tr > td:nth-of-type(7),thead >tr > th:nth-of-type(7){
