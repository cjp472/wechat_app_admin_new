.searchSelectArea {
    background: url('/images/ie-select.png') right center no-repeat;
    background-position: 95% 50%;
    width:  80px;
    height: 34px;
    overflow: hidden; /*隐藏了小三角，因为宽度为110px,而select宽度为130px*/
    border: 1px solid #ccc;
    border-radius: 4px;
}

.searchSelectArea select {
    padding-left: 5px;
    background-color: transparent!important;
    border-width: 0px;
    border-top-style: none;
    border-right-style: none;
    border-left-style: none;
    border-bottom-style: none;
    width:110px;
    display:block;
    height: 34px;
    overflow:hidden;
}

#resource_package{
    width: 330px;
}

.base_topBar{
    z-index: 9000;
}

.modal{
    z-index: 9001;
}

