
.base_right {
    min-width: 1000px;
}

.baseItemWrapper {
    width: 100%;
}

.baseTitleLeft {
    width: 168px;
    height: 36px;
    line-height: 36px;
    padding-left: 40px;
    float: left;
    font-size: 14px;
    color: #353535;
}
.starIcon {
    color: #e64340;
    margin-left: 3px;
}
.baseContentRight {
    float: left;

}
.inputDynamicTitle {
    width: 640px;
}
.redBorder {
    border: 1px solid #F00;
}
.baseItemTitle {
    margin-top: 30px;
    height: 36px;
}

.baseItemContent {
    width: 100%;
    height: 398px;
    margin: 30px 0 0 0;
}
.baseItemFileUpload {
    width: 100%;
    height: 52px;
    margin: 20px 0 0 0;
}
.baseItemServerInfo{
    padding-top: 25px;
    width:100%;
    height:130px;
    clear: both;

}
.resDescribe {
    width: 700px;
}

.uploadFileBtnWrapper {
    height: 36px;
    width: 100%;
}
.uploadFileBtn {
    float: left;
}
input.selectPdfFileInput[type=file] {
    display: none;
}
.uploadFileTip {
    float: left;
    height: 36px;
    line-height: 36px;
    margin: 0 20px 0;
    color: #888;
}

/*********************** 进度条 - start *************************/

.uploadBox {
    float: left;
    width: 100%;
    padding: 10px 0 0 0;
}

.uploadProgress {
    float: left;
    width: 520px;
    height: 40px;
    border-radius: 2px;
    background-color: #fafbfc;
    padding: 0 50px 0 10px;
}
.pdfFileIconWrapper {
    width: 40px;
    height: 40px;
    float: left;
}
.pdfFileIcon {
    width: 100%;
    height: 100%;
}

.uploadPTitle {
    display: block;
    float: left;
    height: 40px;
    line-height: 40px;
    max-width: 500px;
    color: #353535;
    text-align: center;
    overflow: hidden;
    word-wrap: break-word;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.uploadPSize {
    float: left;
    height: 20px;
    margin-left: 10px;
    line-height: 20px;
    color: #b2b2b2;
}

.uploadPLine {
    float: left;
    width: 500px;
    height: 5px;
    margin-top: 17px;
}

.uploadPLineActive {
    float: left;
    width: 0;
    height: 100%;
    background: #2a75ed;
}
.uploadPercent {
    float: left;
    height: 40px;
    line-height: 40px;
    margin-left: 20px;
    color: #2a75ed;
    cursor: pointer;
}
.selectContent{
    width: 600px;
}
/*********************** 进度条 - end *************************/

.baseItemTip {
    margin:10px 0 42px 160px;
    width: 100%;
    font-size: 12px;
    color: #888888;
}

.horizontalDivideLine {
    height: 1px;
    margin: 20px 40px 0 40px;
    background-color: #f2f2f2;
    clear: both;
}

.releaseDynamicBtn {
    margin: 30px 0 0 163px;
}

.remindNum{
    color: #888;
    display: inline-block;
}

.listMsg{
    color: #666;
    padding-left: 24px;
    display: inline-block;
    margin-bottom: 16px;
}















