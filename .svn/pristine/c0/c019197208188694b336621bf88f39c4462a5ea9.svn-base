$baseColor: #f9f9f9;
$basePadding: 20px;

.base_right {
	min-width: 1110px;
}
.base_mainContent{
	padding: 0 0 50px;
	background-color: #efeff4;
	&>div{
		margin-top: 10px;
		background-color: white;
		box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.05);
	}
	.myShop {
		height: 76px;
		padding: 26px 0 0 20px;
		margin-top: 0;
	}
}

.titleText {
	height: 22px;
	line-height: 22px;
	margin: 20px 0 0 20px;
	font-size: 16px;
	font-weight: 600;
	color: #353535;
	a {
		margin: 5px 0 0 10px;
		line-height: 17px;
		height: 17px;
		font-size: 12px;
	}
	.goodsManageTip {
		height: 20px;
		line-height: 20px;
		font-size: 14px;
		color: #888888;
		font-weight: 400;
		margin-left: 10px;
	}
}

.goodCase{
	padding-bottom: 10px;
}
.caseContent{
	width: 100%;
	height: 130px;
	padding: 20px;
	margin-top: 18px;
	& > div{
		position: relative;
		cursor: default;
		float: left;
		width: 80px;
		height: 90px;
		margin-right: 40px;
		text-align: center;
		&>img{
			width: 60px;
			height: 60px;
			border-radius: 50%;
		}
		p{
			margin-top: 10px;
			font-size: 14px;
			text-align: center;
			color: #243042;
		}
		.codeContent{
			display: none;
			position: absolute;
			top: -200px;
			left: -35px;
			width: 150px;
			height: 180px;
			padding: 15px;
			border-radius: 2px;
			background-color: #ffffff;
			box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);
			border: solid 1px #dbdbdb;
			img{
				display: block;
				width: 120px;
				height: 120px;
			}
			p{
				font-size: 12px;
			}
		}
		&:hover .codeContent {
			display: block;
		}
	}
}

.myShop{
	height: 60px;
	width: 100%;
	padding: 12px 0;
	margin-top: $basePadding;
	.content{
		float: left;
		height: 24px;
		line-height: 24px;
		img{
			width: 24px;
			height: 24px;
			float: left;
		}
		.rocket_upgrade{
			width: 10px;
			height: 16px;
			object-fit: contain;
			margin: 4px 5px 0 0;
		}
		p{
			display: inline-block;
			margin: 0 0 0 10px;
			font-size: 18px;
			font-weight: 500;
			color: #353535;
			float: left;
		}
		.version{
			border-radius: 1px;
			background-color: #5e99f7;
			color: #fff;
			font-size: 12px;
			padding: 0 3px;
			line-height: 20px;
			margin: 2px 20px 0 10px;
			float: left;
		}
		a{
			line-height: 24px;
			height: 24px;
			float: left;
		}
	}
	.btnContent{
		float: right;
		position: relative;
		.qrcodeArea{
			display: none;
			position: absolute;
			top: 50px;
			right: 0;
			overflow: hidden;
			width: 180px;
			height: 220px;
			padding: $basePadding 30px;
			border-radius: 2px;
			background-color: #ffffff;
			box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);
			border: solid 1px #dbdbdb;
			text-align: center;
			z-index: 100;
			.text{
				line-height: 20px;
				font-size: 12px;
				color: #243042;
			}
			.codeContent{
				text-align: center;
				margin: 10px 0;
			}
			.href{
				a{text-decoration: underline !important; color: #2a75ed; font-size: 12px;}
			}
		}
	}
	
}

.shopData{
	background-color: #efeff4 !important;
	.shopOperate{
		float: left;
		width: 59%;
		margin-right: 1%;
		background-color: white;
		.content{
			height: 70px;
			margin: 10px 0 18px;
			padding: 0;
			.item{
				float: left;
				height: 100%;
				width: 25%;
				text-align: center;
				border-left: 1px solid #eeeeee;
				.dataText{
					line-height: 17px;
					height: 17px;
					font-size: 12px;
					color: #888888;
					margin-top: 9px;
				}
				.dataNum{
					line-height: 33px;
					height: 33px;
					font-size: 24px;
					color: #3e3e3e;
				}
			}
		}
	}
	.shopAccount{
		float: left;
		background-color: white;
		width: 40%;
		.content{
			height: 70px;
			margin: 10px 0 18px;
			padding: 0;
			.item{
				float: left;
				width: 49%;
				height: 100%;
				text-align: center;
				border-left: 1px solid #eeeeee;
				text-overflow: ellipsis;
				overflow: hidden;
				white-space: nowrap;
				position: relative;
				.itemDesc {
					text-align: left;
					height: 100%;
					margin-left: 20px;
					float: left;
					.dataText{
						line-height: 17px;
						height: 17px;
						font-size: 12px;
						color: #888888;
						margin-top: 9px;
					}
					.dataNum{
						line-height: 33px;
						height: 33px;
						font-size: 24px;
						color: #3e3e3e;
					}
				}
				a {
					position: absolute;
					top: 39px;
					right: 2%;
					float: left;
					width: 56px;
					height: 28px;
					line-height: 28px;
					text-align: center;
					border-radius: 2px;
				}
				.recharge {
					color: #ffffff;
					background-color: #2a75ed;
				}
				.withdrawCash {
					color: #353535;
					background-color: #ffffff;
					border: solid 1px #e5e7eb;
				}


				//.bottomBtn{
				//	display:block;
				//	width: 100%;
				//	line-height: 32px;
				//	text-align: center;
				//}
				//.recharge{
				//	background: #5f99f6;
				//	color: #fff;
				//}
				//.record{
				//	background: #eeeeee;
				//	color: #243042;
				//}
			}
		}
	}
}

//商品管理、营销管理
.coreData{
	padding-bottom: 10px;
	.contentManage{
		width: 100%;
		height: auto;
		margin: 20px 0 0 20px;
		.contentItem{
			margin: 0 10px 10px 0;
			float: left;
			width: 340px;
			height: 170px;
			position: relative;
			border: solid 1px #f0f0f0;
			.itemName{
				height: 20px;
				font-size: 14px;
				font-weight: bold;
				color: #243042;
				margin: 10px 0 10px 10px;
				.hotItem {
					display: inline-block;
					background-color: #ffb85e;
					width: 40px;
					height: 20px;
					line-height: 20px;
					text-align: center;
					margin-left: 15px;
					color: #ffffff;
					font-size: 12px;
					font-weight: normal;
					border-radius: 11px;
				}
				.newItem {
					display: inline-block;
					background-color: #fb6161;
					width: 40px;
					height: 20px;
					line-height: 20px;
					text-align: center;
					margin-left: 15px;
					color: #ffffff;
					font-size: 12px;
					font-weight: normal;
					border-radius: 11px;
				}
			}
			.itemCount{
				display: block;
				position: absolute;
				top: 10px;
				right: 10px;
				width: 172px;
				height: 20px;
				color: #888;
				font-size: 14px;
				text-align: right;
				text-overflow: ellipsis;
				overflow: hidden;
				white-space: nowrap;
			}
			.itemButton{
				display: none;
				position: absolute;
				right: 10px;
				top: 6px;
				width: 138px;
				height: 28px;
				.itemAddBar{
					width: 64px;
					height: 28px;
					line-height: 28px;
					float: left;
					border-radius: 2px;
					border: solid 1px #e5e7eb;
					background-color: #fafbfc;
					color: #353535;
					display: block;
					text-align: center;
					font-size: 14px;
				}
				.itemManageBar{
					width: 64px;
					height: 28px;
					line-height: 28px;
					float: right;
					border: 1px solid #2a75ed;
					border-radius: 2px;
					background-color: #2a75ed;
					color: #fff;
					display: block;
					text-align: center;
					font-size: 14px;
				}
			}
			.itemBorder{
				width: 100%;
				height: 1px;
				background-color: #eee;
			}
			.noData{
				width: 100%;
				.noDataImage {
					width: 48px;
					height: 48px;
					margin: 30px auto 10px;
				}
				.noDataText {
					height: 17px;
					line-height: 17px;
					width: 100%;
					text-align: center;
					font-size: 14px;
					color: #666;
				}
			}
			.itemTitle{
				width: 100%;
				height: 30px;
				text-overflow: ellipsis;
				overflow: hidden;
				white-space: nowrap;
				margin-top: 10px;
				font-size: 14px;
				color: #666666;
				line-height: 30px;
				a {
					color:#666666;
				}
				img {
					float: left;
					width: 24px;
					height: 18px;
					margin: 6px 8px 6px 9px;
				}
			}
			.itemTitle:hover {
				background-color: #f6f8fa;
			}
		}
		.contentItem:hover .itemButton{
			display: block;
		}
		.contentItem:hover  .itemCount{
			display: none;
		}

	}
}

.markingData{

	.markingArea{
		height: 120px;
		margin: 20px 0 20px 20px;
		.markingItem{
			width: 192px;
			height: 120px;
			margin-right: 20px;
			float: left;
			border-radius: 2px;
			background-color: #ff9066;
			line-height: 20px;
			text-align: center;
			color: #fff;
			.markingName{
				margin: 30px auto 20px;
				width: 100%;
				height: 20px;
				font-size: 20px;
			}
			.markingContent{
				width: 100%;
				height: 20px;
			}
		}
	}
}

.moreService{
	width: 100%;
	.serviceArea{
		margin: 20px;
		height: 60px;
		.serviceItem{
			width: 156px;
			height: 60px;
			margin: 0 20px 0 0;
			float: left;
			color: #353535;
			background-color: #fafbfc;
			border: solid 1px #eee;
			.serviceImg{
				float: left;
				height: 24px;
				width: 24px;
				margin: 18px 10px 18px 20px;
				img{
					width: 100%;
					height: 100%;
				}
			}
			.serviceName{
				float: left;
				margin: 20px 0px;
			}
		}
		.serviceItem:hover {

		}
	}
}


.tutorialSection {
	.content {
		width: 100%;
		height: 140px;
		padding: $basePadding;
		.item {
			float: left;
			margin-right: 50px;
			width: 110px;
			height: 100px;
			overflow: hidden;
			text-align: center;
			img {
				width: 80px;
				height: 80px;
			}
			p{
				font-size: 13px;
				text-align: center;
				color: #525252;
				margin:0;
				padding: 0;
				line-height: 20px;
			}
		}
	}
}

a.newHref, a.bottomBtn {
	&:hover{
		box-shadow: inset 0 0 1000px rgba(0, 0, 0, .05);
	}
	&:active{
		box-shadow: inset 0 0 1000px rgba(0, 0, 0, .1);
	}
}

a.item {
	position: relative;
	&:before{
		content: '';
		position: absolute;
		left: 25px;
		top: 10px;
		width: 60px;
		height: 60px;
		border-radius: 50%;
		background: rgba(0,0,0,.05);
		display: none;
	}
	&:hover:before{
		display: block;
	}
	&:active:before{
		background: rgba(0,0,0,.1);
	}

}
/*优惠券介绍，概况页弹窗*/
.welcomeBoxClose{
    position: fixed;
    top:50px;
    right:50px;
    cursor: pointer;
}

.welcomeBoxClose img{
    float: left;
    width: 100%;
    height: 100%;
}

.salerWelcomeBox{
    position: absolute;
    width: 600px;
    height: 400px;
    top: 50%;
    left: 50%;
    margin-left: -300px;
    margin-top: -200px;
    border-radius: 8px;
    background: white;
    overflow: hidden;
}

.welcomeBoxBanner{
    width: 100%;
    height: 200px;
    background: #fffcf7;
    overflow: hidden;
}

.welcomeBoxBanner img{
    display: block;
    width: 318px;
    height: 160px;
    margin-left: auto;
    margin-right: auto;
    margin-top: 40px;
}

.welcomeBoxTitle{
    width: 100%;
    line-height: 22px;
    text-align: center;
    font-size: 16px;
    color: #353535;
    font-weight: 500;
    margin-top: 40px;
}

.welcomeBoxIntro{
    width: 100%;
    line-height: 20px;
    text-align: center;
    font-size: 14px;
    color: #666666;
    margin-top: 10px;
}

.enterIntroBtn{
    width: 160px;
    position: absolute;
    left:50%;
    bottom: 30px;
    margin-left: -80px;
}

.baseIcon{
	position:relative;


}
.baseIcon:after{
	transform: scale(0.5);
	position: absolute;
	right:10px;
	width:64px;
	text-align:center;
	line-height:32px;
	height:32px;
	top:50%;
	margin-top:-8px;
	border-radius: 16px;
	font-size: 20px;
	font-weight: 500;
	display: block;
	color:#fff;
	transform-origin: right top;
}
.baseIcon.newIcon:after{
	content:"New";
	background-color: #fb6161;
}
.baseIcon.hotIcon:after{
	content:"Hot";
	background-color: #ffb85e;
}

.darkScreen2.indWel{

	display: flex;
	align-items: center;
	justify-content: center;
	margin-top:0;
	opacity:0;
	visibility:hidden;
}
.indexWelcomeCloseBtn{
	position: absolute;
	right:60px;
	top:60px;
	cursor: pointer;
}

.darkScreen2.indWel.active{
	opacity:1;
	visibility:visible;
}

.darkScreen2.indWel.ready{
	transition-property: opacity,visibility;
	transition-duration:0.5s;
}

.indexWelcomeBox{
	width:600px;
	height:400px;
	background:#fff;
	position: relative;
	border-radius:8px;
	overflow: hidden;
}

.indexWelcomeBox>.indConBan{
	background:#fffcf7;
	text-align: center;
}

.indexWelcomeCont{
	text-align: center;

}

.indexWelcomeCont>span{
	font-size: 16px;
	color: #353535;
	font-weight: 500;
	margin-top: 40px;
	display: block;
}

.indexWelcomeCont>p{
	font-size: 14px;
	color: #666;
	margin-top: 10px;
	line-height:20px;
	position: relative;
}